'''
   play around with python io
'''

import io

class inpout(object):
    def __init__(self, *args, **kwargs):
        print
        print "Coolnesss"
        return

    def openIO(self, comName):
        print "COM opened: %s" %comName
        self.a = io.open(comName)
        return

    def readIO(self):
        while(1):
            print self.a.readline()
        return
